{
    "name": "oeltimacreation/sistemabsensi-backend",
    "description": "SWAP API Backend",
    "version": "v1.0.0-beta1",
    "license": "proprietary",
    "require": {
        "php": "^8.1",
        "ext-sodium": "*",
        "guzzlehttp/guzzle": "^7.0",
        "jenssegers/agent": "^2.6",
        "lcobucci/clock": "^3.0",
        "lcobucci/jwt": "^5.0",
        "monolog/monolog": "^3.8",
        "mpdf/mpdf": "^8.2",
        "nyholm/psr7": "^1.4",
        "pecee/pixie": "^4.15",
        "phpoffice/phpspreadsheet": "^3.0",
        "pimple/pimple": "^3.2",
        "predis/predis": "^2.0",
        "robmorgan/phinx": "^0.15.5",
        "selective/basepath": "^2.1",
        "slim/http-cache": "^1.1",
        "slim/psr7": "^1.0.0",
        "slim/slim": "^4.8",
        "slim/twig-view": "^3.3",
        "symfony/cache": "^6.0",
        "symfony/dotenv": "^6.0",
        "symfony/http-client": "^5.4",
        "twig/intl-extra": "^3.5"
    },
    "config": {
        "platform": {
            "php": "8.1.24"
        },
        "sort-packages": true,
        "process-timeout": 0,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "serve": [
            "cd public && php -S localhost:1111"
        ],
        "serve-public": [
            "cd public && php -S 0.0.0.0:8080"
        ]
    },
    "require-dev": {
        "deployer/deployer": "^7.3",
        "vimeo/psalm": "^5.20"
    }
}